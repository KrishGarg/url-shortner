generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model shorturls {
  id        String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  longURL   String
  shortURL  String
  uses      Int      @default(0)
  createdAt DateTime @db.Date @default(now())
  updatedAt DateTime @db.Date @updatedAt
}
